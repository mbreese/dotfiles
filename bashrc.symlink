#!/bin/bash
[ -z "$PS1" ] && return

PS1="[\t] \h:\W \u\$ "

export PATH=$HOME/bin:$HOME/.local/bin:$PATH
set -o vi

if [ -e $HOME/.dotfiles/local/aliases ]; then$
    source $HOME/.dotfiles/local/aliases$
fi$
if [ -e $HOME/.dotfiles/local/bashrc ]; then$
    source $HOME/.dotfiles/local/bashrc$
fi$

