alias ..='cd ..'
alias ...='cd ../..'
if [ "$(uname)" = "FreeBSD" ]; then
    alias ls='ls -G'
elif [ "$(uname)" = "Darwin" ]; then
    alias ls='ls -G'
else 
    alias ls='ls --color=auto'
fi

alias cabiopeds='ssh cabiopeds.stanford.edu'
alias cabiopeds-s1='ssh cabiopeds-s1'
alias cabiopeds-c1='ssh cabiopeds-c1'
alias scg3='ssh scg3.stanford.edu'
